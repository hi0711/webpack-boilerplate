{
  "name": "hi0711-design",
  "version": "1.1.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:pug & npm run build:webpack & npm run serve",
    "build:webpack": "npx webpack --watch",
    "build:image": "npx imagemin src/img/**/* -o dist/image; node reduce-webp.js",
    "build": "sed -i '' '/dist/d' .gitignore & sed -i '' '1s/development/production/' webpack.config.js & npm run csscomb & npx webpack; npm run build:image",
    "serve": "npx browser-sync start -s ./dist --port 9000 -f ./dist",
    "pug": "npx pug --hierarchy --pretty -o ./dist ./src/tmpl; find ./dist -name '_*.html' | xargs rm -rf & find ./dist -type d -name '*inc' | xargs rm -rf",
    "watch:pug": "npx watch 'npm run pug' ./src/tmpl/",
    "csscomb": "npx csscomb ./src/scss/**/*.scss"
  },
  "author": "hi0711",
  "license": "Good Boy license",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.6",
    "css-loader": "^0.28.11",
    "csscomb": "^4.2.0",
    "html2pug": "^2.0.1",
    "imagemin": "^5.3.1",
    "imagemin-cli": "^3.0.0",
    "imagemin-webp": "^4.1.0",
    "json-loader": "^0.5.7",
    "modernizr": "^3.6.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.2",
    "pug": "^2.0.1",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "watch": "^1.0.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.0",
    "superagent": "^3.8.2"
  }
}
